<!DOCTYPE html>
<html lang="en">
  <head>
  		<% include ./../partials_template/head %>
  </head>

  <body data-spy="scroll" data-offset="0" data-target="#theMenu"   >

	<!-- <% include ./../partials_template/header %> -->
  	<% include ./../partials_template/menu_templates/menu_sub_playlist %>

	<!-- ========== HEADER SECTION ========== -->
	<section id="home" name="home"></section>
	<div id="headerwrap" style="background:white">

		<div class="container">
			<div class="row" >
        <div class="col-xs-3 ">
            <a href="/playlist/edit"><span class="glyphicon glyphicon-wrench" style="font-size: 12em"></span></a>
        </div>
        <div class="col-lg-3">
        </div>
        <h1 style="color:black; text-align:left;">EDIT playlist</h1>

      <div> 
    </div><!-- /container -->
  </div><!-- /headerwrap -->

        <!-- bootstrap table begin -->
        <div class="container">

          <h2>Current playlist(s)</h2>
        <table class="table table-hover">

        <thead>
          <tr>
           <th style="text-align:center;">ID</th>

         </tr>
        </thead>
          <% playlists.forEach(function(an_playlist){ %>
            <tbody>
            <tr>
              <th style="text-align:center;"> <%= an_playlist.pname %> </th>
            </tr>
            </tbody>
          <% }); %>

        </table>
        </div>
        <!-- bootstrap table END. -->





  	<!-- ========== other scoll SECTION ========== -->



    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
  <footer>
    <% include ../partials_template/footer %>
  </footer>
</body>
</html>
